@{
    ViewData["Title"] = "Home Page";
}

@model BookListViewModel

<h1>Book List</h1>

@foreach (Book p in Model.Books)
{
    <partial name="BookSummary" model="p" />
}

<div page-model="@Model.PaginationInfo" page-action="Index" page-classes-enabled="true" page-class="btn"
     page-class-normal="btn-outline-dark" page-class-selected="btn-prima ry" class="btn-group pull-right m-1"></div>